
@{
    ViewData["Title"] = "Upload Sales Order";
}

<div class="row">
    <div class="col-lg-12">
        <h4 class="heading">Upload <strong>Sales Orders</strong> <span></span></h4>
    </div>
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="form_main">
            <div class="form">
                <form asp-action="UploadSalesOrder" method="post" enctype="multipart/form-data">
                    <span>Browse csv file: </span>
                    <input type="file" name="file" />
                    <input type="submit" value="submit" name="submit" class="txt2">
                </form>
                <a asp-action="DownloadSalesOrderTemplate" class="btn btn-sm btn-info">
                    Download Sales Order File
                </a>
            </div>
        </div>

    </div>
</div>

